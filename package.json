{
  "name": "telegraf-inline-menu",
  "version": "3.2.2",
  "description": "Inline Menus for Telegraf made simple.",
  "main": "source",
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "ava": "ava",
    "ava:watch": "ava -w",
    "nyc": "nyc",
    "xo": "xo",
    "start": "node example.js",
    "pretest": "xo",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdJoPaTo/telegraf-inline-menu.git"
  },
  "keywords": [
    "Telegraf",
    "Telegram"
  ],
  "author": "Edgar To",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EdJoPaTo/telegraf-inline-menu/issues"
  },
  "homepage": "https://github.com/EdJoPaTo/telegraf-inline-menu#readme",
  "dependencies": {
    "telegraf": "^3.26.0"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "nyc": "^13.1.0",
    "xo": "^0.24.0"
  },
  "files": [
    "source/**/*.js",
    "!*.test.js"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "overrides": [
      {
        "files": [
          "source/build-keyboard*.js",
          "source/middleware-helper.is-action.test.js",
          "test/*.js"
        ],
        "rules": {
          "camelcase": [
            "warn",
            {
              "properties": "never"
            }
          ]
        }
      }
    ]
  }
}
