{
  "name": "telegraf-inline-menu",
  "version": "3.3.0",
  "description": "Inline Menus for Telegraf made simple.",
  "main": "dist",
  "engines": {
    "node": ">=8.13"
  },
  "scripts": {
    "ava": "ava",
    "nyc": "nyc",
    "xo": "xo",
    "build": "del dist && tsc",
    "prepare": "npm run build",
    "start": "ts-node example.ts",
    "pretest": "xo",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdJoPaTo/telegraf-inline-menu.git"
  },
  "keywords": [
    "Telegraf",
    "Telegram"
  ],
  "author": "Edgar To",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EdJoPaTo/telegraf-inline-menu/issues"
  },
  "homepage": "https://github.com/EdJoPaTo/telegraf-inline-menu#readme",
  "dependencies": {},
  "peerDependencies": {
    "telegraf": "^3.26.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.2.0",
    "@types/node": "^10.12.21",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "ava": "^1.2.1",
    "del-cli": "^1.1.0",
    "eslint-config-xo-typescript": "^0.7.0",
    "nyc": "^13.2.0",
    "telegraf": "^3.27.0",
    "telegram-typings": "^4.0.0-beta",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1",
    "xo": "^0.24.0"
  },
  "files": [
    "dist",
    "!*.test.*"
  ],
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage",
      "dist",
      "test",
      "**/*.test.*"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "extends": "xo-typescript",
    "extensions": [
      "ts"
    ],
    "space": true,
    "semicolon": false,
    "rules": {
      "ava/no-ignored-test-files": "warn",
      "@typescript-eslint/indent": [
        "error",
        2
      ]
    },
    "overrides": [
      {
        "files": [
          "source/build-keyboard*.*",
          "source/middleware-helper.is-action.test.*",
          "test/*.*"
        ],
        "rules": {
          "@typescript-eslint/camelcase": "off"
        }
      }
    ]
  }
}
