{
  "name": "telegraf-inline-menu",
  "version": "3.3.0",
  "description": "Inline Menus for Telegraf made simple.",
  "main": "dist",
  "engines": {
    "node": ">=8.13"
  },
  "scripts": {
    "ava": "ava",
    "nyc": "nyc",
    "xo": "xo",
    "build": "del dist && tsc",
    "prepare": "npm run build",
    "start": "ts-node example.ts",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdJoPaTo/telegraf-inline-menu.git"
  },
  "keywords": [
    "Telegraf",
    "Telegram"
  ],
  "author": "Edgar To",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EdJoPaTo/telegraf-inline-menu/issues"
  },
  "homepage": "https://github.com/EdJoPaTo/telegraf-inline-menu#readme",
  "dependencies": {},
  "peerDependencies": {
    "telegraf": "^3.26.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.2.0",
    "ava": "^1.1.0",
    "del-cli": "^1.1.0",
    "nyc": "^13.1.0",
    "telegraf": "^3.27.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1"
  },
  "files": [
    "dist",
    "!*.test.*"
  ],
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage",
      "dist",
      "test",
      "**/*.test.*"
    ],
    "all": true,
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
