{
  "name": "telegraf-inline-menu",
  "version": "3.1.0",
  "description": "Inline Menus for Telegraf made simple.",
  "main": "inline-menu.js",
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "ava": "ava",
    "ava:watch": "ava -w",
    "nyc": "nyc",
    "standard-version": "standard-version",
    "xo": "xo",
    "start": "node example.js",
    "coverage": "nyc --reporter=lcov --reporter=text ava",
    "prerelease": "npm test",
    "release": "standard-version",
    "pretest": "xo",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EdJoPaTo/telegraf-inline-menu.git"
  },
  "keywords": [
    "Telegraf",
    "Telegram"
  ],
  "author": "Edgar To",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EdJoPaTo/telegraf-inline-menu/issues"
  },
  "homepage": "https://github.com/EdJoPaTo/telegraf-inline-menu#readme",
  "dependencies": {
    "telegraf": "^3.23.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.8",
    "nyc": "^13.0.1",
    "standard-version": "^4.4.0",
    "xo": "^0.23.0"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "overrides": [
      {
        "files": [
          "build-keyboard*.js",
          "middleware-helper.is-action.test.js",
          "test/*.js"
        ],
        "rules": {
          "camelcase": [
            "warn",
            {
              "properties": "never"
            }
          ]
        }
      }
    ]
  }
}
